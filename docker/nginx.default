server {
    listen 80;
    server_name example.org;
    access_log /dev/stdout;

    location / {
        include uwsgi_params;
        uwsgi_pass ${SEMANTIC_BOT_SVC};
        proxy_set_header Host $host;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    }
    location /static {
        root /usr/src/app;
    }
    location /assets {
        root /usr/src/app;
    }
}
